#ifndef ISTACK_HPP
#define	ISTACK_HPP

#include  "IOperand.hpp"

class IStack
{
public:
 virtual ~IStack() {}

 virtual IOperand * pop() = 0;
 virtual void       push(IOperand *) = 0;
 virtual void       dump() = 0;
 virtual long unsigned int getSize() = 0;
};

#endif	/* ISTACK_HPP */

